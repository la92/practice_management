<p id="notice"><%= notice %></p>
<div class="container-fluid">
  <h1 style="text-align:center">List of Patients</h1>
</div
<div class="container-fluid">
<table class="table table-sm table-dark">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Middle Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Date of Birth</th>
      <th scope="col">Gender</th>
      <th scope="col">SSN</th>
      <th scope="col">Email</th>
      <th scope="col">Phone Number</th>
      <th scope="col">Address Line 1</th>
      <th scope="col">Address Line 2</th>
      <th scope="col">City</th>
      <th scope="col">State</th>
      <th scope="col">Zip Code</th>

    </tr>
  </thead>

  <tbody>
    <% @patients.each do |patient| %>
      <tr>
        <td><%= patient.first_name %></td>
        <td><%= patient.middle_name %></td>
        <td><%= patient.last_name %></td>
        <td><%= patient.dob %></td>
        <td><%= patient.sex %></td>
        <td><%= patient.ssn %></td>
        <td><%= patient.email %></td>
        <td><%= patient.phone_number %></td>
        <td><%= patient.address_line1 %></td>
        <td><%= patient.address_line2 %></td>
        <td><%= patient.city %></td>
        <td><%= patient.state %></td>
        <td><%= patient.zip_code %></td>
        <td><%= link_to 'Show', patient,class:"btn btn-primary" %></td>
        <td><%= link_to 'Edit', edit_patient_path(patient), class:"btn btn-primary" %></td>
        <td><%= link_to 'Destroy', patient, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<button type="button" class="btn btn-light"><%= link_to 'New Patient', new_patient_path %></button>
</div>
